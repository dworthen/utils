{
  "name": "@d-dev/bin-install",
  "version": "0.0.3",
  "description": "Install binaries",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "bin": {
    "bin-install": "./dist/bin.js"
  },
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "build:types": "tsc",
    "build:lib": "tsup",
    "build": "run-p build:types build:lib",
    "start": "tsup --watch"
  },
  "keywords": [
    "bin",
    "binary"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/tar": "^6.1.5",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "extract-zip": "^2.0.1",
    "glob": "^10.3.10",
    "handlebars": "^4.7.7",
    "tar": "^6.1.15",
    "undici": "^5.22.1",
    "used-pm": "^1.1.1"
  }
}
