{
  "name": "@d-dev/eslint-plugin",
  "version": "1.1.0",
  "description": "plugin for enforcing esm or cjs file extensions in imports.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "clean": "shx rm -rf ./dist",
    "build": "tsup --dts",
    "start": "tsup --watch",
    "docs": "api-extractor run --local"
  },
  "author": "Derek Worthen <worthend.derek@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dworthen/utils"
  },
  "homepage": "https://github.com/dworthen/utils",
  "license": "MIT",
  "dependencies": {
    "@sinclair/typebox": "^0.25.1",
    "micromatch": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@microsoft/api-extractor": "^7.33.5",
    "@types/eslint": "^8.4.9",
    "@types/jest": "^29.2.0",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.11.7",
    "@typescript-eslint/parser": "^5.41.0",
    "@typescript-eslint/utils": "^5.41.0",
    "babel-jest": "^29.2.2",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "eslint": "^8.26.0"
  }
}
