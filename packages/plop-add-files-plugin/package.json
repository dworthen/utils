{
  "name": "@d-dev/plop-add-files-plugin",
  "version": "1.2.0",
  "description": "Polp plugin for adding files.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "build": "tsup --dts",
    "start": "tsup --watch",
    "docs": "api-extractor run --local"
  },
  "keywords": [
    "plop",
    "addFiles",
    "scaffold",
    "plugin"
  ],
  "author": "Derek Worthen <worthend.derek@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dworthen/utils"
  },
  "homepage": "https://github.com/dworthen/utils",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.8",
    "plop": "^3.1.1",
    "shx": "^0.3.4",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "plop": "^3.1.1"
  },
  "dependencies": {
    "@d-dev/logger": "workspace:^1.1.0",
    "glob": "^8.0.3"
  }
}
